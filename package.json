{
  "name": "vscode-fuse-syntax",
  "displayName": "Fuse Syntax (Fusetools)",
  "description": "Syntax Highlighting for *.uno and *.ux files ",
  "version": "1.0.1",
  "publisher": "naumovs",
  "engines": {
    "vscode": "^1.6.0"
  },
  "activationEvents": [
    "*"
  ],
  "icon": "preview.jpg",
  "main": "out/main.js",
  "categories": [
    "Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "unoproj",
        "aliases": [
          "Fuse project",
          "unoproj"
        ],
        "extensions": [
          ".unoproj"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "uno",
        "aliases": [
          "Uno",
          "uno"
        ],
        "extensions": [
          ".uno",
          ".unotrace"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "ux",
        "aliases": [
          "UX",
          "ux",
          "fuseux"
        ],
        "extensions": [
          ".ux"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "uno",
        "scopeName": "source.uno",
        "path": "./syntaxes/uno.tmLanguage"
      },
      {
        "language": "ux",
        "scopeName": "text.xml",
        "path": "./syntaxes/ux.tmLanguage"
      },
      {
        "language": "unoproj",
        "scopeName": "text.unoproj",
        "path": "./syntaxes/unoproj.tmLanguage"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "node_modules/.bin/babel src --out-dir out",
    "dev": "babel src --out-dir out --watch",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiirocks/vscode-fuse-syntax.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sergiirocks/vscode-fuse-syntax/issues"
  },
  "homepage": "https://github.com/sergiirocks/vscode-fuse-syntax#readme",
  "dependencies": {
    "vscode": "1.0.0"
  },
  "devDependencies": {
    "@types/node": "^6.0.45",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-plugin-syntax-flow": "^6.13.0",
    "babel-plugin-transform-flow-strip-types": "^6.14.0",
    "babel-plugin-typecheck": "^3.9.0",
    "babel-preset-es2015": "^6.16.0"
  }
}